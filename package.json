    "@types/react": "18.2.74",
    "@typescript-eslint/eslint-plugin": "7.5.0",
    "@typescript-eslint/parser": "7.5.0",
    "eslint": "8.57.0",
    "eslint-config-next": "^14.1.4",
    "eslint-config-prettier": "9.1.0",
    "prettier": "3.2.5",
    "prisma": "5.12.1",
    "typescript": "5.4.3"

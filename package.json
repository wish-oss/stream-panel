    "easypanel.js": "1.1.6",
    "fast-deep-equal": "3.1.3",
    "framer-motion": "11.0.24",
    "jotai": "2.7.2",
    "next": "^14.1.4",
    "next-auth": "4.24.7",
    "nodemailer": "6.9.13",
    "openai-edge": "1.2.2",
    "qs": "6.12.0",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-email": "2.1.1",
    "react-icons": "5.0.1",
    "react-use": "17.5.0",
    "react-virtuoso": "4.7.7",
